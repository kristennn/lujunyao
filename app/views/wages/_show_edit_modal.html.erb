<!-- 编辑工资弹框代码-开始 -->
<div class="modal fade" id="editModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog" style="margin-top:100px;">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
        <h4 class="modal-title" id="myModalLabel">
          编辑工资
        </h4>
      </div> 
      <div class="modal-body">
        <%= form_for @wage do |f| %>
          <div class="star_import_model_file_style">
            <ul class ="star_import_model">
              <li>
                <%= label_tag "本月应发现金：" %>
                <%= f.text_field :gross_cash, required: true %>
              </li> 
              <% if @wage.net_cash.present? %>
                <li>
                  <%= label_tag "本月实发现金：" %>
                  <%= f.text_field :net_cash, required: true %>
                </li>
              <% end %>
              <li>
                <%= label_tag "本月应发易货币：" %>
                <%= f.text_field :gross_virtual_money, required: true %>
              </li>
            </ul>
          </div>
          <div class="" style="">
             <%= submit_tag :点击确定, :class => "btn btn-primary-status" %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
<!-- 编辑工资弹框代码-结束 -->